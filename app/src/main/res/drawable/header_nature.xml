<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="270"
                android:startColor="#FF7ECED4"
                android:endColor="#FF3E7A94" />
        </shape>
    </item>

    <item
        android:width="120dp"
        android:height="120dp"
        android:gravity="top|end"
        android:right="32dp"
        android:top="26dp">
        <shape android:shape="oval">
            <solid android:color="#55FFF5D6" />
        </shape>
    </item>

    <item
        android:gravity="bottom"
        android:height="110dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF3E6D57" />
            <corners
                android:topLeftRadius="120dp"
                android:topRightRadius="120dp" />
        </shape>
    </item>

    <item
        android:gravity="bottom|start"
        android:height="95dp"
        android:width="240dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF2F5B47" />
            <corners
                android:topLeftRadius="100dp"
                android:topRightRadius="140dp" />
        </shape>
    </item>

    <item
        android:gravity="bottom|end"
        android:height="90dp"
        android:width="220dp">
        <shape android:shape="rectangle">
            <solid android:color="#FF254A3B" />
            <corners
                android:topLeftRadius="130dp"
                android:topRightRadius="90dp" />
        </shape>
    </item>
</layer-list>
